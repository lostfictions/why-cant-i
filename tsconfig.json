{
  "compilerOptions": {
    "target": "es2019",
    "lib": [
      "ES2020"
    ],
    "module": "commonjs",
    "outDir": "dist",
    "strict": true,
    "noPropertyAccessFromIndexSignature": true,
    // "noUncheckedIndexedAccess": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "rootDir": "./src",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "skipLibCheck": true
  },
  "include": [
    "src"
  ]
}